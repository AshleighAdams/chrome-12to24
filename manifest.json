{
	"manifest_version": 2,

	"name": "12h to 24h",
	"description": "Convert 12h times to 24h times",
	"icons": {
		"128": "icon_128.png",
		"256": "icon.png"
	},

	"version": "0.0.1",
	"author": "Kobra",

	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["libs.js", "contentscript-end.js"],
			"run_at": "document_end",
			"all_frames": true
		}
	],

	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}
